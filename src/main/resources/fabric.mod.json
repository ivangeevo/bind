{
  "schemaVersion": 1,
  "id": "bind",
  "version": "${version}",
  "name": "Bind!",
  "description": "Place your tools as blocks in the world",
  "authors": [
    "ivangeevo",
    "RatherBeLunar"
  ],
  "contact": {
    "repo": "https://github.com/ivangeevo/bind"
  },
  "license": "CC-BY-4.0",
  "icon": "assets/bind/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "org.bind.client.BindModClient"
    ],
    "main": [
      "org.bind.BindMod"
    ],
    "fabric-datagen": [
      "org.bind.BindModDataGenerator"
    ]
  },
  "mixins": [
    "bind.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
